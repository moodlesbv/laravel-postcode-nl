{
    "name": "speelpenning/laravel-postcode-nl",
    "description": "A Laravel client using the Postcode.nl REST API for Dutch address verification.",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerben Speelpenning",
            "email": "gerben@speelpenning.nl"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/config": "11.*",
        "illuminate/contracts": "11.*",
        "illuminate/routing": "11.*",
        "illuminate/support": "11.*",
        "illuminate/validation": "11.*"
    },
    "require-dev": {
        "laravel/laravel": "11.*",
        "phpunit/phpunit": "^11.0.1",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Speelpenning\\PostcodeNl\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Speelpenning\\PostcodeNl\\PostcodeNlServiceProvider"
            ]
        }
    }
}
